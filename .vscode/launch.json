    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}\\webpack-dev-server"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Jest",
            "program": "${workspaceFolder}\\node_modules\\jest\\bin\\jest.js"
        },
        {
            "name": "npm scripts",
            "type": "node",
            "request": "attach",
            "port": 5858,
            "address": "localhost",
            "restart": false,
            "sourceMaps": false,
            "localRoot": "${workspaceRoot}",
            "protocol":"auto"

        }
    ]
}